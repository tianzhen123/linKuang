summerready=function(){
	getUserInfo();
	$(function() {
        			var list = [
        				{
        					content: "https://dj.lykyjt.com:8898/image/gH_banner1.png"
        				}, 
        				{
        					content: "https://dj.lykyjt.com:8898/image/gH_banner2.png"
        				}, 
        			];
        			var islider = new iSlider({
        				type: 'pic',
        				data: list,
        				dom: document.getElementById("iSlider-wrapper"),
        				isLooping: true,
        				animateType: 'default',
        				isAutoplay: true,
        				animateTime: 800
        			});
        			islider.addDot();
        		})
}

