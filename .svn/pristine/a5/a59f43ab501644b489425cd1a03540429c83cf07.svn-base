<!DOCTYPE html>
<html>
	<head>
		<meta name="viewport" content="width=device-width, minimum-scale=1, maximum-scale=1">
		<meta charset="UTF-8">
		<title></title>
		<!-- 默认 start -->
		<link rel="stylesheet" href="../../css/iuapmobile.um.css">
		<link rel="stylesheet" href="../../css/font-icons.css">	
		<!-- 默认 end -->
		
		<!-- 头部 start -->
		<link rel="stylesheet" href="../../css/base.css" />
		<!-- 头部 end -->
		
		<!-- 样式修改  start-->
		<link rel="stylesheet" href="bangFuJiLuXiangQing.css"/>
		<!-- 样式修改  end-->

		<!-- 默认 start -->
		<script src="../../js/summer.js" ></script>		
		<script src="../../js/jquery.min.js" ></script>
		<script src="../../js/Frameworks/iuapmobile.frameworks.ui.js" ></script>
		<script src="../../js/knockout.js"></script>
		<script src="../../js/hp/hp_slide.js"></script>
		<script src="../../html/bangFuJiLu/bangFuJiLuXiangQing.js" ></script>
		<!-- 默认 end -->
		
	</head>
	<body>
		<div class="um-win" id="index" name="hp_slide">
			<!-- 头部  start-->
			<div class="um-header">
            	<a href="#"  class="um-back" onclick="backClick()">返回</a>
            	<h3>帮扶记录详情</h3>  
            </div>
			<!-- 头部  end-->
			<!-- data-bind="foreach:data" -->
			<!-- 内容体 start-->
			<div class="um-content" >
				<!-- 帮扶申请表 start-->
                <ul class="um-list">
                    <li class="um-list-item">
                		<div class="um-list-item-inner">
                			<div class="um-list-item-left pl15">
                				姓名:
                			</div>
                			<div class="um-list-item-right" data-bind="text:name">
                				
                			</div>
                		</div>
                	</li>
                	<li class="um-list-item">
                		<div class="um-list-item-inner">
                			<div class="um-list-item-left pl15">
                				所属党组织:
                			</div>
                			<div class="um-list-item">
                				<div data-bind="text:pk_partyorg_name" style="padding: 0 10px;" class="form-control tl" >
                					
                				</div>
                			</div>
                		</div>
                	</li>
<!--                 	<li class="um-list-item">
                		<div class="um-list-item-inner">
                			<div class="um-list-item-left pl15">
                				党员编号
                			</div>
                			<div class="um-list-item-right">
                				<input type="text" class="form-control tr"  placeholder="1120921">
                			</div>
                		</div>
                	</li> -->                	
                	<li class="um-list-item">
                		<div class="um-list-item-inner">
                			<div class="um-list-item-left pl15">
                				性别:
                			</div>
                			<div class="um-list-item-right">
                				<div data-bind="text:sex" class="form-control tr"></div>
                			</div>
                		</div>
                	</li>
                	<li class="um-list-item">
                		<div class="um-list-item-inner">
                			<div class="um-list-item-left pl15">
                				身份证号:
                			</div>
                			<div class="um-list-item-right">
                				<div data-bind="text:idcode" class="form-control tr"></div>
                			</div>
                		</div>
                	</li>
                	<li class="um-list-item">
                		<div class="um-list-item-inner">
                			<div class="um-list-item-left pl15">
                				部门:
                			</div>
                			<div class="um-list-item-right">
                				<div data-bind="text:department" class="form-control tr"></div>
                			</div>
                		</div>
                	</li>
                	
                	<li class="um-list-item">
                		<div class="um-list-item-inner">
                			<div class="um-list-item-left pl15">
                				岗位:
                			</div>
                			<div class="um-list-item-right">
                				<div data-bind="text:post" class="form-control tr"></div>
                			</div>
                		</div>
                	</li>
<!--                 	<li class="um-list-item">
                		<div class="um-list-item-inner">
                			<div class="um-list-item-left pl15">
                				入党时间
                			</div>
                			<div class="um-list-item-right">
                				<input type="text" class="form-control tr" placeholder="请输入入党时间">
                			</div>
                		</div>
                	</li> -->
                	
                	<li class="um-list-item">
                		<div class="um-list-item-inner">
                			<div class="um-list-item-left pl15">
                				困难等级:
                			</div>
                			<div class="um-list-item-right">
                				<div data-bind="text:difficultgrade" class="form-control tr"></div>
                			</div>
                		</div>
                	</li>

                	<li class="um-list-item">
                		<div class="um-list-item-inner">
                			<div class="um-list-item-left pl15">
                				困难建立时间:
                			</div>
                			<div class="um-list-item-right">
                				<div data-bind="text:difficulttime" class="form-control tr"></div>
                			</div>
                		</div>
                	</li>
                	
                	<li class="um-list-item">
                	    <div class="um-list-item-inner">
                			<div class="um-list-item-left pl15">
                				状态:
                			</div>
                			<div class="um-list-item-right">
                				<div data-bind="text:def1" class="form-control tr"></div>
                			</div>
                		</div>
					</li>
                	
                   <li class="um-list-item">
                		<div style="width: 98%">
                		                  		
                			<div style="margin-top: 8px">&nbsp;&nbsp;困难情况描述:</div>
                			
                			<div class="um-list-item">
                				<div style="padding: 0 3%;text-indent:30px;min-height: 100px">
                					<div data-bind="text:difficultinfo">

                					</div>
                				</div>
                			</div>
                			<p style="padding: 0 3%;" id="frame1" ></p>
                			
                			<hr style="border-color: #ececec; border-width: 1px; border-style: solid;" /> 
                		</div>
                	</li>
              	
                	<li class="um-list-item" >
                		<div  style="width: 98%">
                		    <div>&nbsp;&nbsp;备注:</div>
                			
                			<div class="um-list-item">
                				<div style="padding: 0 3%;text-indent:30px;min-height: 100px">
                					<div data-bind="text:remarks" >

                					</div>
                				</div>
                			</div>
                			<p style="padding: 0 3%;" id="frame2" ></p>
                			
                		</div>
                	</li>
                </ul>
				<!-- 帮扶申请表 end-->
			</div>
			<!-- 内容体 end-->
			
			
			
		</div>
	</body>
</html>