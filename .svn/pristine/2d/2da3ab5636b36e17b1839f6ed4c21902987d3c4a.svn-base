*{margin: 0;padding: 0;}
.um-header{
	background-color: #d72424;
	display: flex;
  	justify-content: center;
  	align-items: center;
  	height: 64px;
  	padding-top: 20px;
}
.header_img{
	width: 30%;
	margin:auto;
}
*{
	font-size: 1.0rem;
}
/*列表 start*/
.listview-detail {
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
}
.list_top{
	margin-left: 7px;
	margin-right: 12px;
}
/*列表 end*/
/*图标 start*/
.swipe {
    overflow: hidden;
    visibility: hidden;
    position: relative;
    background: #fff;
    padding-bottom: 10px;
}

.swipe-wrap {
    overflow: hidden;
    position: relative;
}

.swipe-wrap .swipe-box {
    float: left;
    width: 100%;
    position: relative;
}

.sliderow {
    text-align: center;
    width: 100%;
    display: block;
    overflow: hidden;
}

.sliderow .slidecol {
    float: left;
    width: 24.5%;
    height: 95px;
}

.sliderow .slidecol img {
    width: 60%;
    margin-top: 10px;
}

.sliderow .slidecol div {
    font-size: 13px;
    margin-top: 5px;
    padding: 0px 5px;
}
/*图标 end*/
   
   /*轮播 start*/
.iSlider-wrapper {
	height: 180px;
}
.islider-pic{
	height: 100% !important;
}
#banner{
	height: 150px;
}
#banner img{
	width: 100%;
	height: auto;
}
.body-content{
	display:flex;
	flex-direction:column;
}
.cont-desc{
	flex:1;
}
#time{
	text-align: left;
	margin-top: 10px;
}
.um-content {
    position: relative;
}
.mescroll{
	position: absolute;
	top: 0;
	bottom: 0;
	height: auto;/*如设置bottom:51px,则需height:auto才能生效*/
	overflow: auto;
}
.mescroll-upwarp{
	height:0;
	padding:0 !important;
}
