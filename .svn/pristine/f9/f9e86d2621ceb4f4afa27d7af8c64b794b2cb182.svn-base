*{
	margin: 0;
	padding: 0;
	font-size:0.3rem;
}
.um-content{
	background: #f2f2f2;
	padding: 10px;
}
.content-body{
	width: 100%;
	height: 100%;
	
}
.body-head{
	display: flex;
	background-color: #fff;
}
.body-head div{
	flex:1;
	text-align: center;
	margin:0.2rem 0;
	position: relative;
	border-right: 1px solid #f6f6f6;
}
.body-head div:last-child{
	border-right: 0px;
}
.body-head-img{
	position:relative;
	width: 0.6rem;
}
.body-head div span{
	position: absolute;
    right: 0.3rem;
    top: -0.1rem;
    background-color: #fe898b;
    border-radius: 0.5rem;
    color: #fff;
    font-size: 0.3rem;
    width: 0.5rem;
}
.body-head div p{
	padding-top:10px;
	font-size: 12px;
}
.body-content-ul{
	background: #fff;
	display:flex;
	flex-wrap:wrap;
}
.body-content-ul li{
	width: 33.333%;
	padding:10px;
	text-align:center;
	border-right: 1px solid #f6f6f6;
	border-bottom:1px solid #f6f6f6;
}
.body-content-ul li img{
	width:1rem;
}
