*{
	margin: 0;
	padding: 0;
}
.um-content{
	background: #f2f2f2;
	padding: 10px;
}
.content-body{
	width: 100%;
	height: 100%;
	
}
.body-head{
	display: flex;
	background-color: #fff;
}
.body-head div{
	flex:1;
	text-align: center;
	padding:0.2rem 0;
	position: relative;
	border-right: 1px solid #f6f6f6;
}
.body-head div:last-child{
	border-right: 0px;
}
.body-head-img{
	position:relative;
	width: 0.6rem;
}
.body-head div span{
	position: absolute;
	right: 0.4rem;
	top:0.2rem;
	background-color: #BE2426;
	border-radius: 1rem;
	color:#fff;
	font-size:0.3rem;
}
.body-head div p{
	padding-top:10px;
	font-size: 12px;
}
