<!DOCTYPE html>
<html>
	<head>
		<meta name="viewport" content="width=device-width, minimum-scale=1, maximum-scale=1">
		<meta charset="UTF-8">
		<title></title>
		<!-- 默认 start -->
		<link rel="stylesheet" href="../../css/iuapmobile.um.css">
		<link rel="stylesheet" href="../../css/font-icons.css">	
		<!-- 默认 end -->
		
		<!-- 头部 start -->
		<link rel="stylesheet" href="../../css/base.css" />
		<!-- 头部 end -->
		
		<!-- 样式修改  start-->
		<link rel="stylesheet" href="shenQingTianXie.css"/>
		<!-- 样式修改  end-->

		<!-- 默认 start -->
		<script src="../../js/summer.js" ></script>		
		<script src="../../js/jquery.min.js" ></script>
		<script src="../../js/Frameworks/iuapmobile.frameworks.ui.js" ></script>
		<!-- 默认 end -->
	</head>
	<body>
		<div class="um-win" id="index">
			<!-- 头部  start-->
			<div class="um-header">
            	<a href="#"  class="um-back" onclick="backClick()">返回</a>
            	<h3>帮扶申请</h3>  
            </div>
			<!-- 头部  end-->
			
			<!-- 内容体 start-->
			<div class="um-content">
				<!-- 帮扶申请表 start-->
                <ul class="um-list">
                	<li class="um-list-item">
                		<div class="um-list-item-inner">
                			<div class="um-list-item-left pl15">
                				所属党组织
                			</div>
                			<div class="um-list-item-right">
                				<input type="text" class="form-control tr" placeholder="请输入所属党组织">
                			</div>
                		</div>
                	</li>
                	<li class="um-list-item">
                		<div class="um-list-item-inner">
                			<div class="um-list-item-left pl15">
                				党员编号
                			</div>
                			<div class="um-list-item-right">
                				<input type="text" class="form-control tr"  placeholder="1120921">
                			</div>
                		</div>
                	</li>
                	<li class="um-list-item">
                		<div class="um-list-item-inner">
                			<div class="um-list-item-left pl15">
                				党员姓名
                			</div>
                			<div class="um-list-item-right">
                				<input type="text" class="form-control tr" placeholder="请输入党员姓名">
                			</div>
                		</div>
                	</li>
                	<li class="um-list-item">
                		<div class="um-list-item-inner">
                			<div class="um-list-item-left pl15">
                				性别
                			</div>
                			<div class="um-list-item-right">
                				<input type="text" class="form-control tr" placeholder="男/女">
                			</div>
                		</div>
                	</li>
                	<li class="um-list-item">
                		<div class="um-list-item-inner">
                			<div class="um-list-item-left pl15">
                				身份证号
                			</div>
                			<div class="um-list-item-right">
                				<input type="text" class="form-control tr" placeholder="请输入身份证号">
                			</div>
                		</div>
                	</li>
                	<li class="um-list-item">
                		<div class="um-list-item-inner">
                			<div class="um-list-item-left pl15">
                				工作单位
                			</div>
                			<div class="um-list-item-right">
                				<input type="text" class="form-control tr" placeholder="请输入工作单位">
                			</div>
                		</div>
                	</li>
                	<li class="um-list-item">
                		<div class="um-list-item-inner">
                			<div class="um-list-item-left pl15">
                				工作岗位
                			</div>
                			<div class="um-list-item-right">
                				<input type="text" class="form-control tr" placeholder="请输入工作岗位">
                			</div>
                		</div>
                	</li>
                	<li class="um-list-item">
                		<div class="um-list-item-inner">
                			<div class="um-list-item-left pl15">
                				入党时间
                			</div>
                			<div class="um-list-item-right">
                				<input type="text" class="form-control tr" placeholder="请输入入党时间">
                			</div>
                		</div>
                	</li>
                	<li class="um-list-item">
                		 <div class="um-label um-box-justify um-list-item-inner">
	                    	<div>困难类型</div>
	                    	<div>
	                    		<span class="mr15">离职农村干部党员</span><i class="ti-angle-right"></i>
	                    	</div>
	                    </div>
                	</li>
                   <li class="um-list-item">
                		<div class="um-list-item-inner">
                			<div class="um-list-item-body" >
	                			<div class="vt pl15 um-row">
	                				困难情况描述
	                			</div>
	                			<div class="vt pl15 um-row">
	                				<textarea name="" id="" cols="37" rows="10" class="form-control i-form-control"></textarea>
	                			</div>
	                		</div>
                		</div>
                	</li>

                	<li class="um-list-item">
                		<div class="um-list-item-inner">
                			<div class="um-list-item-left pl15">
                				申请时间
                			</div>
                			<div class="um-list-item-right">
                				<input type="text" class="form-control tr" placeholder="请输入申请时间">
                			</div>
                		</div>
                	</li>
                	
                	<li class="um-list-item">
                		<div class="um-list-item-inner">
                			<div class="um-list-item-body" >
	                			<div class="vt pl15 um-row">
	                				备注
	                			</div>
	                			<div class="vt pl15 um-row">
	                				<textarea name="" id="" cols="37" rows="10" class="form-control i-form-control"></textarea>
	                			</div>
	                		</div>
                		</div>
                	</li>
                </ul>
				<!-- 帮扶申请表 end-->
			</div>
			<!-- 内容体 end-->
			
			<!-- 底部 start-->
			<div class="um-footer">
                <div class="um-footer">
                    <div class="um-footerbar">
                        <a href="#" class="um-footerbar-item">                         
                           <button class="btn btn-inline">查看审批情况</button>
                        </a>                        
                        <a href="#" class="um-footerbar-item">
                             <button class="btn-red btn-inline">收回</button>
                        </a>
                    </div>
                </div>
			</div>
			<!-- 底部 end-->
			
		</div>
	</body>
</html>