/* 导航 文字*/
.um-tabbar-underline > li.active > a {
    background-color: transparent;
    color: #FF0000;
}
/* 导航 下划线*/
.um-tabbar-underline > li.active {
    color: #007aff;
    background-color: transparent;
    border-bottom: 2px solid #FF0000;
}

/* 链接 默认设置 */
a {
    color: #000;
    text-decoration: none;
    cursor: pointer;
}

/* 内容显示   显示active的*/
div.Artical>div {
	display:none;
	color:#555;
}
div.Artical>div.active {
	display:block;
}











